<Project>
	<Import Project="..\..\.Global\Build\Build.targets" />
	<PropertyGroup>
		<BuildProperty>build-property-value</BuildProperty>
	</PropertyGroup>
	<PropertyGroup>
		<BuildDependsOn>
			$(BuildDependsOn);
			Test;
			GlobalBuildTarget;
			BuildTarget;
		</BuildDependsOn>
	</PropertyGroup>
	<Target Name="BuildTarget">
		<Message Importance="high" Text="BuildTarget: BuildProperty = $(BuildProperty)" />
	</Target>
	<Target Name="Test">
		<Message Importance="high" Text="- Information " />
		<Message Importance="high" Text="   - ImportDirectoryBuildProps = $(ImportDirectoryBuildProps)" />
		<Message Importance="high" Text="   - ImportDirectoryBuildTargets = $(ImportDirectoryBuildTargets)" />
		<Message Importance="high" Text="   - DirectoryBuildPropsPath = $(DirectoryBuildPropsPath)" />
		<Message Importance="high" Text="   - DirectoryBuildTargetsPath = $(DirectoryBuildTargetsPath)" />
		<Message Importance="high" Text="   - _DirectoryBuildPropsFile = $(_DirectoryBuildPropsFile)" />
		<Message Importance="high" Text="   - _DirectoryBuildPropsBasePath = $(_DirectoryBuildPropsBasePath)" />
		<Message Importance="high" Text="   - MSBuildProjectDirectory = $(MSBuildProjectDirectory)" />
		<Message Importance="high" Text="   - MSBuildProjectDirectory = $(MSBuildProjectDirectory)" />
		<Message Importance="high" Text="   - SolutionDir = $(SolutionDir)" />
	</Target>
</Project>